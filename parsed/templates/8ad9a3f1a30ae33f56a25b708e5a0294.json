{"components/common/footer":"<footer class=\"footer\" role=\"contentinfo\">\r\n    <div class=\"quicklink-bar\">\r\n        <div class=\"container\">\r\n            {{#if customer}} \r\n                <ul>\r\n                    <li class=\"quick\"><a href=\"/resources/quick-order/\">Quick Order</a></li>\r\n                    <li class=\"order\"><a href=\"/resources/check-order-status/\">Check Order Status</a></li>\r\n                    <li class=\"invoice\"><a href=\"/resources/pay-invoice/\">Pay Invoice</a></li>\r\n                    <li class=\"account\"><a href=\"/account.php?action=order_status\">My Account</a></li>\r\n                </ul>\r\n            {{else}}\r\n                 <ul>\r\n                    <li class=\"quick\"><a href=\"/resources/quick-order/\">Quick Order</a></li>\r\n                    <li class=\"order\"><a href=\"/resources/check-order-status/\">Check Order Status</a></li>\r\n                    <li class=\"invoice\"><a href=\"/resources/pay-invoice/\">Pay Invoice</a></li>\r\n                    <li class=\"account\"><a href=\"{{urls.auth.create_account}}\">Create Account</a></li>\r\n                </ul>\r\n            {{/if}}\r\n        </div>\r\n    </div>\r\n    <div class=\"footer-bottom\">\r\n    <div class=\"container\">       \r\n        <section class=\"footer-info\">\r\n            {{#if customer}}\r\n            <article class=\"footer-info-col footer-info-locator\">\r\n                <h5 class=\"footer-info-heading\">Your Territory Manager</h5>\r\n                <div class=\"TM_Detail\">\r\n                        <img class=\"TM_Image\" src=\"\" alt=\"Territory Manager\"/>\r\n                        <p class=\"NameMn\"><strong class=\"TM_name\"></strong></p>\r\n                        <span class=\"PhoneMn\"><a href=\"\" class=\"TM_phone\"></a></span>\r\n                        <span class=\"EmailMn\"><a href=\"\" class=\"TM_email\"></a></span>\r\n                    </div>\r\n            </article> \r\n            {