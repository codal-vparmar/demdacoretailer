de ************/\r\n\t})}})();\r\n</script>\r\n\r\n</footer>\r\n<script type=\"text/javascript\" src=\"https://www.demdacoretailers.com/content/quick-order/base64.js\"></script> \r\n<script type=\"text/javascript\" src=\"https://www.demdacoretailers.com/content/quick-order/qoapi.js\"></script> \r\n<script type=\"text/javascript\">\r\n$(\".search-div .is-srOnly\").click(function(){\r\n  $(\".form-field\").slideToggle();\r\n});\r\n$(\".artistcategory .LeftCategories ul li.firstlevel > .icon\").click(function(){\r\n  $(this).parent().toggleClass('active');\r\n  $(this).next(\".subnavList\").slideToggle();\r\n});\r\n$(document).ready(function() {\r\n$(\".back-arw\").hide();\r\n//$(\".navPage-subMenu\").hide();\r\n\r\n\r\n$(\".navPages-item.has-subMenu .navPages-action-moreIcon\").click(function(){  \r\n  $(this).next().show().addClass('menuactive');\r\n  $(this).next().find (\".back-arw\").show();\r\n  $('.navPages').addClass('active');\r\n  $(this).parent('.navPages-item.has-subMenu').addClass('active');\r\n});\r\n\r\n\r\n$(\".back-arw\").click(function(){  \r\n  $(this).parent().parent().hide().removeClass('menuactive');;\r\n  $('.navPages').removeClass('active');\r\n  $(this).parent('.navPages-item.has-subMenu').removeClass('active');\r\n  \r\n});\r\n\r\n$(\".level-2 .back-arw\").click(function(){  \r\n  $(this).parent().parent().parent().hide().removeClass('menuactive');;\r\n  $('.navPages').removeClass('active');\r\n  $(this).parent('.navPages-item.has-subMenu').removeClass('active');\r\n  \r\n});\r\n$(\".level-3 .back-arw\").click(function(){  \r\n  $(this).parent().parent().parent().hide().removeClass('menuactive');;\r\n  $('.navPages').removeClass('active');\r\n  $(this).parent('.navPages-item.has-subMenu').removeClass('active');\r\n  \r\n});\r\n$(\".level-4 .back-arw\").click(function(){  \r\n  $(this).parent().parent().parent().hide().removeClass('menuactive');;\r\n  $('.navPages').removeClass('active');\r\n  $(this).parent('.navPages-item.has-subMenu').removeClass('active');\r\n  \r\n});\r\n$(\".level-5 .back-arw\").click(function(){  \r\n  $(this).parent().parent().parent().hide().removeClass('menuactive');;\r\n  $('.navPages').removeClass('active');\r\n  $(this).parent('.navPages-item.has-subMenu').removeClass('active');\r\n  \r\n});\r\n\r\n$(document).on('click', '.custom_toggle .toggle_btn', function(e){\r\n            e.preventDefault();\r\n            $('.custom_toggle .toggle_btn').removeClass('is-open');\r\n            $('.toggle_content.is-active').removeClass('is-active').slideUp();\r\n            $(this).addClass('is-open');\r\n            $(this).next('.toggle_content').addClass('is-active').slideDown();           \r\n        });\r\n});\r\n\r\n// Redirect url after login \r\n/*$(document).ready(function(){\r\n    var userlogin = $('body').hasClass('CustomerNotSignIn'); \r\n    if(userlogin==true){\r\n        if(window.location.href!='https://www.demdacoretailers.com/login.php'){\r\n            // Store\r\n            sessionStorage.setItem(\"lasturl\",window.location.href);\r\n        }\r\n    }\r\n\r\n    if(userlogin==false){\r\n        var lastURL = sessionStorage.getItem(\"lasturl\");\r\n        if(lastURL!=''){\r\n            sessionStorage.setItem(\"lasturl\",'');\r\n             // Retrieve\r\n            window.location.href=lastURL;\r\n        }\r\n    }        \r\n}); */\r\n</script>\r\n\r\n<!-- Header Nav [start] -->\r\n<script type='text/javascript'>\r\n$(document).ready(function(){\r\n$(\".head-left #shopcategory\").hover(\r\n  function () {\r\n    $('.navPage-subMenu').stop().animate({\"opacity\": \"1\"}, \"fast\");  \r\n    $('#shopcategory div.navPage-subMenu').css({\"visibility\":\"visible\"});\r\n  },\r\n  function () {\r\n   $('.navPage-subMenu').stop().animate({\"opacity\": \"0\"}, \"fast\");  \r\n   $('#shopcategory div.navPage-subMenu').css({\"visibility\":\"hidden\"});\r\n  }\r\n);\r\n\r\n$('ul.navPage-subMenu-list.level-1').on('mouseleave', function() {\r\n $('#shopcategory div.navPage-subMenu').css({\"visibility\":\"hidden\"});\r\n});\r\n\r\n\r\n});\r\n</script>\r\n\r\n\r\n<script>\r\n$(window).bind(\"load resize scroll\",function(e){\r\n var textH = $(\".bsh\").outerHeight();\r\n var vertAlign = ((53 + textH));\r\n var svertAlign = ((53 + textH));\r\n $(\"#menu .navPage-subMenu\").css({ 'top' : vertAlign });\r\n $(\"#menu\").css({ 'top' : svertAlign });\r\n $(\"#menu .navPage-subMenu\").css( { height: 'calc(100% - ' + vertAlign + 'px)' } );\r\n});\r\n</script>\r\n<!-- Header Nav [CLOSE] -->\r\n\r\n\r\n<script type=\"text/javascript\" src=\"https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.9.0/slick.js\"></script>\r\n\r\n<script type=\"text/javascript\">\r\nvar categorynm = '{{category.name}}';\r\n//console.log(categorynm);\r\n$.ajax({\r\n    url: \"https://dev-demdaco.stage-codal.net/index.php/api/v1/categoryslider?category=\"+categorynm,\r\n    method: \"GET\",\r\n    contentType: false,\r\n   success: function (data) {\r\n      if(data!=''){\r\n           $('#categorySlider').css({'display':'block'});    \r\n       }\r\n      $('#categorySlider').append(data);\r\n        $('#categorySlider').html( $('.brandGrid', $(data)).html() );\r\n        $('#categorySlider').slick({\r\n         \"dots\": true,        \r\n         \"infinite\": false,\r\n         \"mobileFirst\": true,\r\n         \"arrows\": true,\r\n         \"slidesToShow\": 3,\r\n         \"slidesToScroll\": 3,\r\n         \"responsive\": [\r\n            {\r\n               \"breakpoint\": 1279,\r\n               \"settings\": {\r\n                  \"slidesToShow\": 3,\r\n                  \"slidesToScroll\": 3\r\n               }\r\n            },\r\n            {\r\n               \"breakpoint\": 599,\r\n               \"settings\": {\r\n                  \"slidesToShow\": 3,\r\n                  \"slidesToScroll\": 3\r\n               }\r\n            },\r\n            {\r\n               \"breakpoint\": 0,\r\n               \"settings\": {\r\n                  \"slidesToShow\": 2,\r\n                  \"slidesToScroll\": 2,\r\n                  \"arrows\": false\r\n               }\r\n            }\r\n         ]\r\n      });\r\n   },\r\n    error: function (e) {\r\n      console.log(e);\r\n    }\r\n});\r\n\r\n// search TM using zipcode\r\n$(document).on('click', '#submitTM', function(e){\r\n      $(\".error\").remove();\r\n      $zipcod = $('#zipcode').val();\r\n      if($zipcod!=''){\r\n          get_tm_data($zipcod);  \r\n          return false;\r\n      }else{\r\n         $('#zipcode').after('<div class=\"error\" style=\"color:#9a3324\">Please enter valid zipcode.</div>');\r\n         return false;\r\n      }\r\n});\r\n\r\n\r\n\r\nfunction get_tm_data($zipcod){\r\n   $.ajax({\r\n               url: 'https://dev-demdaco.stage-codal.net/index.php/api/v1/territorymanager',\r\n               method: 'POST',\r\n               data: { zipcode: $zipcod},  // 17232\r\n               dataType: 'json',\r\n               crossDomain: true,\r\n               async: true,\r\n               success: function (data) {\r\n                    if(data.TMReturnStatus=='Success'){\r\n                        $('.BeforeSearch').css({\"display\":\"none\"});\r\n                        $('.DetailsTM').css({\"display\":\"block\"});\r\n                        $('.TM_name').html(data.TMName);\r\n                        $('.TM_phone').html(data.TMPhoneNumber);\r\n                        $('.TM_phone').attr(\"href\",\"tel:\"+data.TMPhoneNumber);\r\n                        $('.TM_email').html(data.TMEmail);\r\n                        $('.TM_email').attr(\"href\",\"mailto:\"+data.TMEmail);\r\n                        if(data.TMPhotoURL){\r\n                            $('.TM_Image').attr(\"src\",data.TMPhotoURL);\r\n                        }else{\r\n                            $('.TM_Image').css({\"display\":\"none\"});\r\n                        }\r\n                        \r\n\r\n                    }else{\r\n                       $('.DetailsTM').css({\"display\":\"none\"});\r\n                       $('#zipcode').after('<div class=\"error\" style=\"color:#9a3324\">Please enter another zipcode.</div>');\r\n                    }\r\n                },\r\n                error: function (e) {\r\n                    //error\r\n                    console.log(e);\r\n                }\r\n\r\n         });\r\n}\r\n\r\n\r\n\r\nvar custid ='{{customer.id}}';;\r\nvar auth = demo();\r\n\r\nfunction getDemDecoData(getCustomerURL){\r\nvar data;\r\n$.ajax({\r\n      url: getCustomerURL,\r\n      method: 'GET',\r\n      dataType: 'json',\r\n      crossDomain: true,\r\n      async: false,\r\n      beforeSend : function(req) {  \r\n         req.setRequestHeader('Authorization', auth);\r\n      },\r\n      success: function(response){\r\n         try{\r\n            data = response[0].zip;\r\n         }catch(error){\r\n            data = '0';\r\n         }  \r\n      }\r\n   });\r\n   return data;\r\n}\r\nvar getCustomerURL = \"/api/v2/customers/\"+custid+\"/addresses.json\";\r\n//var getCustomerURL = \"/api/v2/customers/80.json\";\r\n$user_zip = getDemDecoData(getCustomerURL);\r\n\r\nif($user_zip!='0'){\r\n    get_tm_data($user_zip);\r\n}\r\n\r\n// global legacy API function\r\nfunction globalLegacyAPI(apiURL){\r\n    var data;\r\n    $.ajax({\r\n          url: apiURL,\r\n          method: 'GET',\r\n          dataType: 'json',\r\n          crossDomain: true,\r\n          async: false,\r\n          beforeSend : function(req) {  \r\n             req.setRequestHeader('Authorization', auth);\r\n          },\r\n          success: function(response){\r\n             try{\r\n                data = response;\r\n             }catch(error){\r\n                data = '0';\r\n             }  \r\n          }\r\n       });\r\n       return data;\r\n}\r\n\r\n// Call customer API.\r\nif(custid!=''){\r\n    var apiUrl = \"/api/v2/customers/\"+custid+\".json\";\r\n    $customerRes = globalLegacyAPI(apiUrl);\r\n    $notes = $customerRes.notes;\r\n    if($notes)\r\n      $notes_aray = JSON.parse($notes);\r\n\r\n    if($notes=='' || $notes_aray.on_hold =='false' || $notes_aray.on_hold == false){\r\n      $('body').addClass('accountActive');\r\n    } else if($notes_aray.on_hold =='true' || $notes_aray.on_hold == true ){\r\n      $('body').addClass('accountHold');\r\n    }\r\n\r\n}\r\n\r\n</script>","components/common/payment-icons":"{{#with theme_settings}}\r\n    {{#or show_accept_amex show_accept_discover show_accept_mastercard show_accept_paypal show_accept_visa show_accept_amazonpay show_accept_googlepay}}\r\n    <div class=\"footer-payment-icons\">\r\n        {{#if show_accept_amex}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-american-express\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_discover}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-discover\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_mastercard}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-mastercard\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_paypal}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-paypal\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_visa}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-visa\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_amazonpay}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-amazonpay\"></use></svg>\r\n        {{/if}}\r\n        {{#if show_accept_googlepay}}\r\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-googlepay\"></use></svg>\r\n        {{/if}}\r\n    </div>\r\n    {{/or}}\r\n{{/with}}\r\n","components/common/social-links":"{{#if social_media}}\r\n    <ul class=\"socialLinks socialLinks--alt\">\r\n        {{#each social_media}}\r\n            <li class=\"socialLinks-item\">\r\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\r\n                    <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\r\n                </a>\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n{{/if}}\r\n"}
